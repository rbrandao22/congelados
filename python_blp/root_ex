    # test scipy inside class
    def fun(self, x):
        self.x = x
        return [self.x[0] + .5 * (self.x[0] - self.x[1])**3 - 1.,
               .5 * (self.x[1] - self.x[0])**3 + self.x[1]]

    def call(self):
        x = np.zeros(2)
        sol = optimize.root(self.fun, x)
        print(sol.x)

inst.call